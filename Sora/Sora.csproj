<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>net5.0</TargetFramework>
    <AssemblyVersion>0.1.1.0</AssemblyVersion>
    <FileVersion>0.1.1.0</FileVersion>
    <Version>0.1.1</Version>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Description>.Net 5异步机器人框架(OneBot协议，同时支持miraigo扩展协议)</Description>
    <Copyright>Copyright © 2020 CBGan</Copyright>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageProjectUrl>https://github.com/CBGan/Sora</PackageProjectUrl>
    <Authors>CBGan</Authors>
    <Company></Company>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>TRACE</DefineConstants>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="EventArgs\OnebotEvent\ApiEvent\**" />
    <Compile Remove="TypeEnum\**" />
    <EmbeddedResource Remove="EventArgs\OnebotEvent\ApiEvent\**" />
    <EmbeddedResource Remove="TypeEnum\**" />
    <None Remove="EventArgs\OnebotEvent\ApiEvent\**" />
    <None Remove="TypeEnum\**" />
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Fleck" Version="1.1.0" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="System.Reactive" Version="4.4.1" />
  </ItemGroup>

</Project>
